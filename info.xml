<?xml version="1.0"?>
<extension key="be.ctrl.labeling" type="module">
  <file>labeling</file>
  <name>CiviCRM labeling</name>
  <description>Adds action to generate labels based on message templates</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Hechtermans Wouter</author>
    <email>wouter@ctrl.be</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/kewljuice/be.ctrl.labeling</url>
    <url desc="Documentation">https://github.com/kewljuice/be.ctrl.labeling</url>
    <url desc="Support">http://www.ctrl.be/</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-02-28</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>Once installed 'Labeling: generate labels' action will be available to generate labels based on message
        templates.
    </comments>
  <civix>
    <namespace>CRM/ctrl/Labeling</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <mixins>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
